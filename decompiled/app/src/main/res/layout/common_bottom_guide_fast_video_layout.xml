<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="center_horizontal"
    android:id="@+id/fast_video_layout"
    android:layout_width="@dimen/common_bottom_guide_common_image_width"
    android:layout_height="@dimen/common_bottom_guide_common_image_height">
    <com.oplus.camera.p172ui.widget.TextureVideoView
        android:id="@+id/videoView"
        android:layout_width="@dimen/common_bottom_guide_common_image_width"
        android:layout_height="@dimen/common_bottom_guide_common_image_height"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/iv_video_thumb"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"/>
</FrameLayout>
