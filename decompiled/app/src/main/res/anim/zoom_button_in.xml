<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <alpha
        android:interpolator="@anim/zoom_interpolator_point_alpha"
        android:duration="400"
        android:startOffset="100"
        android:fromAlpha="0"
        android:toAlpha="1"/>
    <translate
        android:interpolator="@anim/zoom_interpolator_in"
        android:duration="400"
        android:startOffset="100"
        android:fromYDelta="25%"
        android:toYDelta="0"/>
</set>
